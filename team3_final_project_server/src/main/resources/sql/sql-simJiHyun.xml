<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.team3_final_project_server.SimJiHyun.mapper.SJHMapper">
  <select id="hasUser" resultType="int">
    select count(*) from user where user_id = #{userId} and user_pass = #{userPass}
  </select>

  <select id="getUserData">
    select * from user where user_id = #{userId} and user_pass = #{userPass}
  </select>

  <select id="findByUserId" parameterType="String" resultType="com.example.team3_final_project_server.dto.UserDTO">
    SELECT * FROM user WHERE user_id = #{userId}
  </select>
</mapper>